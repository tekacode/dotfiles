[
   {
    "key": "s",
    "command": "findJump.activate",
    "when": "vim.mode == 'Normal' && editorFocus"
  },
  {
    "key": "ctrl+;",
    "command": "-findJump.activate",
    "when": "vim.mode == 'Normal' && editorFocus"
  },  
  {
    "key": "cmd+k",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+a",
    "command": "-editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift shift",
    "command": "workbench.action.quickOpen",
    "when": "editorFocus"
  },
  {
    "key": "shift shift",
    "command": "workbench.action.quickOpen",
    "when": "!editorFocus"
  },
  {
    "key": "cmd+right",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "alt+cmd+right",
    "command": "-workbench.action.nextEditor"
  },

  {
    "key": "j",
    "command": "type",
    "args": { "text": "j" },
    "when": "editorTextFocus && vim.active && !inDebugRepl && !suggestWidgetVisible && !findWidgetVisible && vim.mode == 'Normal'"
  },
  {
    "key": "k",
    "command": "type",
    "args": { "text": "k" },
    "when": "editorTextFocus && vim.active && !inDebugRepl && !suggestWidgetVisible && !findWidgetVisible && vim.mode == 'Normal'"
  },
  {
    "key": "j k",
    "command": "extension.vim_escape",
    "when": "editorTextFocus && vim.active && !inDebugRepl && !suggestWidgetVisible && !findWidgetVisible && vim.mode == 'Insert'"
  },
 {
    "key": "cmd+right",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && isWeb"
  },
  {
    "key": "cmd+f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && isWeb"
  },
  {
    "key": "cmd+right",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "cmd+left",
    "command": "workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
    // Run selected code in terminal
  {
  "key": "cmd+r",
  "command": "workbench.action.terminal.runSelectedText",
  "when": "editorTextFocus && editorHasSelection"
 }
]
